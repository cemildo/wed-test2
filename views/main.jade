html
  head
    link(href='//netdna.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css', rel='stylesheet')
    link(href='//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css', rel='stylesheet') 
    script(src='//code.jquery.com/jquery-1.10.2.min.js')
    script(src='//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js')
    script(src='javascripts/main.js')  
    link(rel='stylesheet', type='text/css', href='stylesheets/main.css')
  body
    .container
      .row
      |  
      span.input-group-btn.logout
        button.btn.btn-primary.btn-lg(type='submit') Logout
      |  
      .col-md-6
          h2 
          |             
          #custom-search-input
            .input-group.col-md-12
              input#linkInput.form-control.input-lg(type='text', placeholder='')
              |                     
              span.input-group-btn
                button#addLink.btn.btn-primary.btn-sm(type='submit') Add Link
      |       
      |       
      .col-md-8
        p.lead     
          ul#savedList.list-group
            each val in links
               li(id=val.id).list-group-item= val.name +" Website " +    val.url +"    "
                  button.btn.btn-default.btn-sm(type='submit') delete

                